<scenefile>
	<globaldata>
		<diffusecoeff v="0.5" />
		<specularcoeff v="0.5" />
		<ambientcoeff v="0.5" />

		<framerate fps="30" />
		<duration seconds="10" />
	</globaldata>

	<object type="tree" name="root">
		<transblock>
			<keyframe type="fractional" key="0">
				<translate id="0" x="0" y="3" z="3" />
			</keyframe>

			<cameradata>
				<keyframe type="fractional" key="0">
					<look x="0" y="-3" z="-3" />
					<up x="0" y="1" z="0" />
					<heightangle v="30" />
				</keyframe>
			</cameradata>
		</transblock>

		<transblock>
			<keyframe type="fractional" key="0">
				<translate id="0" x="0" y="1" z="1" />
			</keyframe>

			<lightdata id="0" type="point">
				<keyframe type="fractional" key="0">
					<color r="2.5" g="2.5" b="2.5" />
					<function v1="0.5" v2="0" v3="0" />
				</keyframe>
			</lightdata>
		</transblock>

		<transblock>
			<keyframe type="fractional" key="0">
				<translate id="2" x="-1" y="0" z="0" />
				<rotate id="0" x="0" y="1" z="0" angle="45" />
				<scale id="1" x="0.75" y="0.75" z="0.75" />
			</keyframe>

			<keyframe type="fractional" key="0.5">
				<translate id="2" x="1" y="0" z="0" />
			</keyframe>

			<keyframe type="fractional" key="1">
				<translate id="2" x="-1" y="0" z="0" />
			</keyframe>

			<object type="primitive" name="cube">
				<diffuse r="1" g="1" b="1" />
				<specular r="1.0" g="1.0" b="1.0" />
				<shininess v="25.0" />
				<reflective r="5" g="1" b="1" />
			</object>
		</transblock>
	</object>





<transblock>
	<keyframe type="fractional" key="0">
		<rotate id="0" x="0" y="1" z="0" angle="0" />
	</keyframe>

	<keyframe type="fractional" key="0.25">
		<rotate id="0" x="0" y="1" z="0" angle="180" />
	</keyframe>

	<keyframe type="fractional" key="1">
		<rotate id="0" x="0" y="1" z="0" angle="-180" />
	</keyframe>

	<object type="primitive" name="cube">
		<diffuse r="1" g="1" b="1" />
		<specular r="1.0" g="1.0" b="1.0" />
		<shininess v="25.0" />
		<reflective r="5" g="1" b="1" />
	</object>
</transblock>







</scenefile>
