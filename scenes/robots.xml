<scenefile>
    <globaldata>
        <diffusecoeff v="0.5"/>
        <specularcoeff v="0.5"/>
        <ambientcoeff v="0.5"/>

		<duration seconds="5" />
		<framerate fps="30" />
    </globaldata>

	<object type="tree" name="robot">
		<transblock>
			<keyframe type="fractional" key="0">
				<translate id="8" x="0" y="1" z="-.5"/>
				<scale id="9" x="0.5" y="0.5" z="0.5"/>
			</keyframe>
			
			<object type="tree">
				<transblock>
					<object type="primitive" name="cone" >
						<diffuse r="1" g="1" b="0"/>
					</object>
				</transblock>
				<transblock>
					<keyframe type="fractional" key="0">
						<scale id="10" x="1" y="0.5" z="1"/>
						<translate id="11" x="0" y="-1.5" z="0"/>
					</keyframe>

					<object type="primitive" name="cylinder" >
						<diffuse r="1" g="1" b="0"/>
					</object>
				</transblock>
				<transblock>
					<keyframe type="fractional" key="0">
						<scale id="12" x="0.3" y="0.3" z="0.3"/>
						<translate id="13" x="0" y="-2.3" z="2"/>
					</keyframe>
					
					<object type="primitive" name="cone" >
						<diffuse r="1" g="0" b="0"/>
					</object>
				</transblock>
				<transblock>
					<keyframe type="fractional" key="0">
						<scale id="14" x="0.2" y="0.2" z="0.2"/>
						<translate id="15" x="1" y="-2.9" z="2"/>
					</keyframe>
					
					<object type="primitive" name="sphere" >
						<diffuse r="0" g="0" b="0"/>
					</object>
				</transblock>
				<transblock>
					<keyframe type="fractional" key="0">
						<scale id="16" x="0.2" y="0.2" z="0.2"/>
						<translate id="17" x="-1" y="-2.9" z="2"/>
					</keyframe>
					
					<object type="primitive" name="sphere" >
						<diffuse r="0" g="0" b="0"/>
					</object>
				</transblock>
			</object>
		</transblock>

		
		<transblock>
			<keyframe type="fractional" key="0">
				<translate id="18" x="0" y="0" z="-.5"/>
			</keyframe>
			
			<object type="tree">
				<transblock>
					<keyframe type="fractional" key="0">
						<scale id="19" x="1" y="1" z="0.5"/>
					</keyframe>
					
					<object type="primitive" name="cube" >
						<diffuse r=".5" g=".5" b=".5"/>
					</object>
				</transblock>

				<!-- left arm  -->
				<transblock>
					<keyframe type="fractional" key="0">
						<scale id="20" x=".3" y=".3" z=".3"/>
						<translate id="21" x="2" y="1.5" z="0"/>
					</keyframe>
					
					<object type="tree">
						<transblock>
							<keyframe type="fractional" key="0">
								<rotate id="27" x="1" y="0" z="0" angle="-45"/>
							</keyframe>

							<keyframe type="fractional" key="0.25">
								<rotate id="27" x="1" y="0" z="0" angle="45"/>
							</keyframe>

							<keyframe type="fractional" key="0.5">
								<rotate id="27" x="1" y="0" z="0" angle="-45"/>
							</keyframe>

							<keyframe type="fractional" key="0.75">
								<rotate id="27" x="1" y="0" z="0" angle="45"/>
							</keyframe>

							<keyframe type="fractional" key="1">
								<rotate id="27" x="1" y="0" z="0" angle="-45"/>
							</keyframe>
							
							<object type="tree">
								<transblock>
									<object type="primitive" name="sphere" >
										<diffuse r="0.5" g="0.5" b="0.5"/>
									</object>
								</transblock>
								<transblock>
									<keyframe type="fractional" key="0">
										<scale id="23" x="1" y="2" z="1"/>
										<translate id="24" x="0" y="-.5" z="0"/>
									</keyframe>
									
									<object type="primitive" name="cylinder" >
										<diffuse r=".5" g=".5" b=".5"/>
									</object>
								</transblock>
							</object>
						</transblock>
					</object>
				</transblock>

				<!-- right arm -->
				<transblock>
					<keyframe type="fractional" key="0">
						<scale id="25" x=".3" y=".3" z=".3"/>
						<translate id="26" x="-2" y="1.5" z="0"/>
					</keyframe>
					
					<object type="tree">
						<transblock>
							<keyframe type="fractional" key="0">
								<rotate id="27" x="1" y="0" z="0" angle="45"/>
							</keyframe>

							<keyframe type="fractional" key="0.25">
								<rotate id="27" x="1" y="0" z="0" angle="-45"/>
							</keyframe>

							<keyframe type="fractional" key="0.5">
								<rotate id="27" x="1" y="0" z="0" angle="45"/>
							</keyframe>

							<keyframe type="fractional" key="0.75">
								<rotate id="27" x="1" y="0" z="0" angle="-45"/>
							</keyframe>

							<keyframe type="fractional" key="1">
								<rotate id="27" x="1" y="0" z="0" angle="45"/>
							</keyframe>


							
							<object type="tree">
								<transblock>
									<object type="primitive" name="sphere" >
										<diffuse r="0.5" g="0.5" b="0.5"/>
									</object>
								</transblock>
								<transblock>
									<keyframe type="fractional" key="0">
										<scale id="28" x="1" y="2" z="1"/>
										<translate id="29" x="0" y="-.5" z="0"/>
									</keyframe>
									
									<object type="primitive" name="cylinder" >
										<diffuse r=".5" g=".5" b=".5"/>
									</object>
								</transblock>
							</object>
						</transblock>
					</object>
				</transblock>
			</object>
		</transblock>

		<transblock>
			<keyframe type="fractional" key="0">
				<scale id="30" x=".5" y=".5" z=".5"/>
				<translate id="31" x="0" y="-1" z="-1"/>
			</keyframe>
			
			<object type="tree">
				<transblock>
					<object type="primitive" name="sphere" >
						<diffuse r="0" g="1" b="1"/>
					</object>
				</transblock>
				<transblock>
					<keyframe type="fractional" key="0">
						<rotate id="32" x="1" y="0" z="0" angle="30"/>
						<scale id="33" x="0.8" y="2.2" z="1"/>
						<translate id="34" x="0.8" y="-0.6" z="0"/>
					</keyframe>

					<keyframe type="fractional" key="0.25">
						<rotate id="32" x="1" y="0" z="0" angle="-30"/>
					</keyframe>

					<keyframe type="fractional" key="0.5">
						<rotate id="32" x="1" y="0" z="0" angle="30"/>
					</keyframe>

					<keyframe type="fractional" key="0.75">
						<rotate id="32" x="1" y="0" z="0" angle="-30"/>
					</keyframe>

					<keyframe type="fractional" key="1">
						<rotate id="32" x="1" y="0" z="0" angle="30"/>
					</keyframe>

					<object type="primitive" name="cylinder" >
						<diffuse r="0" g="1" b="1"/>
					</object>
				</transblock>
				<transblock>
					<keyframe type="fractional" key="0">
						<rotate id="35" x="1" y="0" z="0" angle="-30"/>
						<scale id="36" x="0.8" y="2.2" z="1"/>
						<translate id="37" x="-0.8" y="-0.6" z="0"/>
					</keyframe>
					
					<keyframe type="fractional" key="0.25">
						<rotate id="35" x="1" y="0" z="0" angle="30"/>
					</keyframe>

					<keyframe type="fractional" key="0.5">
						<rotate id="35" x="1" y="0" z="0" angle="-30"/>
					</keyframe>

					<keyframe type="fractional" key="0.75">
						<rotate id="35" x="1" y="0" z="0" angle="30"/>
					</keyframe>

					<keyframe type="fractional" key="1">
						<rotate id="35" x="1" y="0" z="0" angle="-30"/>
					</keyframe>

					<object type="primitive" name="cylinder" >
						<diffuse r="0" g="1" b="1"/>
					</object>
				</transblock>
				</object>
		</transblock>
	</object>

    <object type="tree" name="root">
        <transblock>
            <keyframe type="fractional" key="0">
                <translate id="38" x="5" y="2" z="0"/>
            </keyframe>

            <cameradata>
				<keyframe type="fractional" key="0">
					<look x="-5" y="-2" z="0"/>
					<up x="0" y="1" z="0"/>
					<heightangle v="45"/>
				</keyframe>
            </cameradata>
        </transblock>

        <transblock>
            <keyframe type="fractional" key="0">
                <translate id="39" x="4" y="4" z="4"/>
            </keyframe>

            <lightdata type="point" id="0">
				<keyframe type="fractional" key="0">
					<color r="1" g="1" b="1"/>
					<function v1="1" v2="0" v3="0" />
				</keyframe>
            </lightdata>
        </transblock>

        <transblock>
            <keyframe type="fractional" key="0">
                <translate id="40" x="-3" y="3" z="3"/>
            </keyframe>

            <lightdata type="point" id="1">
                <keyframe type="fractional" key="0">
					<color r="1" g="1" b="1"/>
					<function v1="1" v2="0" v3="0" />
				</keyframe>
            </lightdata>
        </transblock>
        
        <transblock>
            <keyframe type="fractional" key="0">
                <translate id="41" x="-3" y="3" z="-3"/>
            </keyframe>

            <lightdata type="point" id="2">
				<keyframe type="fractional" key="0">
					<color r="1" g="1" b="1"/>
					<function v1="1" v2="0" v3="0" />
				</keyframe>
            </lightdata>
        </transblock>

		<transblock>
			<keyframe type="fractional" key="0">
                <translate id="42" x="3" y="3" z="-3"/>
            </keyframe>

			<lightdata type="point" id="3">
				<keyframe type="fractional" key="0">
					<color r="1" g="1" b="1"/>
					<function v1="1" v2="0" v3="0" />
				</keyframe>
            </lightdata>
		</transblock>

		<transblock>
			<keyframe type="fractional" key="0">
				<translate id="43" x="0" y="0" z="-5" />
			</keyframe>

			<keyframe type="fractional" key="1">
				<translate id="43" x="0" y="0" z="5" />
			</keyframe>

			<object type="tree">
				<transblock>
					<object type="master" name="robot" />
				</transblock>

				<transblock>
					<keyframe type="fractional" key="0">
						<translate id="44" x="-1.5" y="0" z="-0.2"/>
					</keyframe>
					

					<object type="master" name="robot" />
				</transblock>

				<transblock>
					<keyframe type="fractional" key="0">
						<translate id="45" x="-3" y="0" z="-0.4"/>
					</keyframe>
					

					<object type="master" name="robot" />
				</transblock>

				<transblock>
					<keyframe type="fractional" key="0">
						<translate id="44" x="-4.5" y="0" z="-0.6"/>
					</keyframe>
					

					<object type="master" name="robot" />
				</transblock>
			</object>


			
		</transblock>
            
    </object>

</scenefile>
